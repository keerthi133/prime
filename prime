#include<stdio.h>
int prime(int ,int);
int main()
{
int num,check;
printf("enter the number:");
scanf("%d",&num);
check=prime(num,num/2);
if (check==1)
{
printf("%d is a prime",num);
}
else
{
printf("%d is not a prime",num);
}

return 0;
}
int prime(int num,int i)
{
if(i==1)
{
return 1;
}
else
{
if(num%i==0)
{
return 0;
}
else
{
return(num,i-1);
}
}
}

